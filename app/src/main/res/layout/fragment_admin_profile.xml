    <data>
        <variable
            name="sharedVM"
            type="com.wagit.desktrack.ui.admin.viewmodel.SharedViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
